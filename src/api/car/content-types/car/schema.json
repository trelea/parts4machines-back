    },
    "body": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::body.body",
      "inversedBy": "cars"
    },
    "color": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::color.color",
      "inversedBy": "cars"
    },
    "fuel": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::fuel.fuel",
      "inversedBy": "cars"
    },
    "traction": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::traction.traction",
      "inversedBy": "cars"
    },
    "wheel": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::wheel.wheel",
      "inversedBy": "cars"
    },
    "transmission": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::transmission.transmission",
      "inversedBy": "cars"
    }
  }
}
