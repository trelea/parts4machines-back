      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "engine_capacity": {
      "type": "integer"
    },
    "horse_power": {
      "type": "integer"
    },
    "seats": {
      "type": "integer"
    },
    "stock": {
      "type": "boolean",
