      "minLength": 17,
      "unique": false
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "images": {
      "type": "media",
